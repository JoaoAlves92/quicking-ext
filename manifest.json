{
  "manifest_version": 3,
  "name": "Quickin Helper",
  "version": "1.0",
  "description": "Helps Vini extract data from Quickin",
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
		{
		 "matches": ["<all_urls>"],
		 "js": ["scripts/xlsx.full.min.js"]
		}
	]
}
